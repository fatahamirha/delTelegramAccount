<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.telegram.android.beta"
          android:versionCode="1"
          android:versionName="1.0">

    <permission android:name="org.telegram.android.beta.permission.C2D_MESSAGE"
                android:protectionLevel="signature" />
    <uses-permission android:name="org.telegram.android.beta.permission.C2D_MESSAGE" />

    <application>
        <receiver android:name="org.telegram.android.gcm.GCMBroadcastReceiver"
                  android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <category android:name="org.telegram.android.beta"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>